    <h3 className=\"panel-title text-center bg-gradient-primary \">\n\n       <div class=\"card N shadow p-1 mb-3 bg-white rounded  \">\n           <div class=\"card-body \">\n              Disha English Medium School,Ichalkaranji\n            </div>\n           </div>\n            </h3>\n          </div>\n          <div className=\"panel-body\">\n           <div className=\"d-flex justify-content-between\">\n            <h4><Link to=\"/create\" className=\"btn btn-primary\">Add Student</Link></h4>\n            <h4 className=\"btn btn-danger\" onClick={onLogout}>Logout</h4>\n             </div> \n\n            <table className=\"table table-stripe table-hover table-bordered shadow-lg p-3 mb-5 bg-white rounded\">\n              <thead className=\"thead-dark\">\n                <tr>\n                  <th>Name</th>\n                  <th>Address</th>\n                  <th>Contact</th>\n                </tr>\n              </thead>\n              <tbody>\n                {data.map((data,i) =>\n                  <tr key={i}>\n                    <td>  <Link to={`/show/${data.id}`}>{data.name}</Link>   </td>\n                    <td>{data.address}</td>\n                    <td>{data.contact}</td>\n                  </tr>\n                )}\n              </tbody>\n            </table>\n          </div>\n        </div>\n      </div>\n    \n       \n    );\n}"},"lineCount":null}},"error":null,"hash":"120683dbe01fb2de0834282fceed3ed2","cacheData":{"env":{}}}